    private static void tilin(boolean p1, boolean p2, boolean p3){
        
      if (p1 == true){
      int PQ1 = 8250;
      } else {
          int PQ1 = 0;
      }
      
       if (p2 == true){
      int PQ2 = 11790;
      } else {
          int PQ2 = 0;
      }
       
       if (p3 == true){
      int PQ3 = 10450;
      } else {
          int PQ3 = 0;
      }
}

