    private void jtuserActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
    }                                      

    private void bloginActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
        if (jtuser.getText().equals("")){
            JOptionPane.showMessageDialog(null, "Please fill out username");
        }
        else if (jpassword.getText().equals("")){
            JOptionPane.showMessageDialog(null, "Please fill out password");
        }
        else if (jtuser.getText().contains("user")&& jpassword.getText().contains("password")){
            general frame2 = new general();
            frame2.setVisible(true);
            dispose();
            
        }
        else {
            JOptionPane.showMessageDialog(null, "Wrong username or password", "Message", JOptionPane.ERROR_MESSAGE);          
        }
    }                                      

    private void jshowpassActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        if (jshowpass.isSelected()){
            jpassword.setEchoChar((char)0);
        }
        else {
            jpassword.setEchoChar('*');
        }
    }                                         

    private void hexitActionPerformed(java.awt.event.ActionEvent evt) {                                      
            System.exit(0);
    }                                     

